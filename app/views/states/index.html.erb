
<h1> All States </h1>

<%= render 'search' %>
<div class="state-index">
    <ul>
        <% @states.each do |s| %>
            <li>
                <br>
                <br>
            <%= s.name %> <%= link_to "Check it out!", state_path(s.id), class: "btn" %> 
            </li>
        <% end %>
    </ul>
</div>